{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 100
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "CreatedAt": "0001-01-01T00:00:00Z",
            "Requester": {}
        },
        "Spec": {
            "Deal": {
                "Concurrency": 1
            },
            "Docker": {
                "EnvironmentVariables": [
                    "{{if .SeedEnv}}{{.SeedEnv}}{{else}}\"RANDOM_SEED=42\"{{end}}"
                ],
                "Entrypoint": [
                    "bash", "-c", "sh /app/train.sh /input /output 'a photo of a sks dog' 200"
                ],
                "Image": "docker.io/marshath/sdxl-finetune:v1.0"
            },
            "Engine": "Docker",
            "Language": {
                "JobContext": {}
            },
            "Network": {
                "Type": "None"
            },
            "PublisherSpec": {
                "Type": "Estuary"
            },
            "Resources": {
                "GPU": "1"
            },
            "Timeout": 1800,
            "Verifier": "Noop",
            "Wasm": {
                "EntryModule": {}
            },
            "inputs": [
                {
                    "CID": "{{.ImagesCID}}",
                    "Name": "lora_input",
                    "StorageSource": "IPFS",
                    "path": "/input"
                }
            ],
            "outputs": [
                {
                    "Name": "output",
                    "StorageSource": "IPFS",
                    "path": "/output"
                }
            ]
        }
    }
}
